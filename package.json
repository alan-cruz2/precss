{
  "name": "precss",
  "version": "1.4.0",
  "description": "Use Sass-like markup in your CSS",
  "keywords": [
    "postcss",
    "css",
    "postcss-plugin",
    "sass",
    "variables",
    "conditionals",
    "ifs",
    "thens",
    "elses",
    "fors",
    "nesteds",
    "nestings",
    "eaches",
    "mixins",
    "imports",
    "extends",
    "placeholders"
  ],
  "author": "Jonathan Neal <jonathantneal@hotmail.com>",
  "license": "CC0-1.0",
  "repository": {
    "type": "git",
    "url": "https://github.com/jonathantneal/precss.git"
  },
  "bugs": {
    "url": "https://github.com/jonathantneal/precss/issues"
  },
  "homepage": "https://github.com/jonathantneal/precss",
  "dependencies": {
    "postcss": "latest",
    "postcss-advanced-variables": "latest",
    "postcss-atroot": "latest",
    "postcss-color-function": "latest",
    "postcss-custom-media": "latest",
    "postcss-custom-properties": "latest",
    "postcss-custom-selectors": "latest",
    "postcss-extend": "latest",
    "postcss-media-minmax": "latest",
    "postcss-mixins": "latest",
    "postcss-nested": "latest",
    "postcss-nesting": "latest",
    "postcss-partial-import": "latest",
    "postcss-property-lookup": "latest",
    "postcss-selector-matches": "latest",
    "postcss-selector-not": "latest"
  },
  "devDependencies": {
    "eslint": "^1.7.3",
    "tap-spec": "^4.1.0",
    "tape": "^4.2.2"
  },
  "scripts": {
    "lint": "eslint . --ignore-path .gitignore",
    "fixtures": "tape test/*.js | tap-spec",
    "test": "npm run lint && npm run fixtures"
  },
  "engines": {
    "iojs": ">=2.0.0",
    "node": ">=0.12.0"
  }
}
